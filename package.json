{
  "name": "beautify-json",
  "displayName": "Beautify JSON",
  "description": "Beautify JSON effortlessly.",
  "version": "1.0.0",
  "icon": "icon.png",
  "repository": {
    "type": "git",
    "url": "git@github.com:randrade23/vscode-prettify-json.git"
  },
  "publisher": "randrade23",
  "engines": {
    "vscode": "^1.30.0"
  },
  "categories": [
    "Programming Languages",
    "Linters"
  ],
  "activationEvents": [
    "onCommand:extension.beautifyJSON"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.beautifyJSON",
        "title": "Beautify JSON"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.1.34",
    "mocha": "^2.3.3",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  },
  "dependencies": {
    "jsonlint": "^1.6.2",
    "strip-json-comments": "^3.0.0"
  }
}
